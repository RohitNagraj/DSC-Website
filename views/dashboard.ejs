    <section id="body-dashboard">
      <div id="accordion container">
        <div class="card">
          <div class="card-header" id="headingOne">
            <h5 class="mb-0">
              <button class="btn btn-link" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true"
                aria-controls="collapseOne">
                Add Blog
              </button>
            </h5>
          </div>

          <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
            <div class="card-body">
              <section id="blog_post">
                <br><br>
                <div class="container">
                  <div class="form-group">
                    <fieldset class="the-fieldset">
                      <legend class="the-legend">Update Blog</legend>
                      <form>
                        <div class="form-group">
                          <label for="exampleFormControlInput1">Title of blog</label>
                          <input type="text" class="form-control" id="title" placeholder="Enter your title here..">
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlTextarea1">Enter the description</label>
                          <textarea class="form-control" id="exampleFormControlTextarea1" rows="25"
                            placeholder="Type your blog here"></textarea>
                        </div>
                        <form>
                          <fieldset class="the-fieldset">
                            <div class="form-group">
                              <label for="exampleFormControlFile1">Upload Image</label>
                              <input type="file" class="form-control-file modify" id="exampleFormControlFile1"
                                accept="image/*">
                            </div>
                          </fieldset>

                        </form><br>
                        <form>
                          <fieldset class="the-fieldset">
                            <div class="form-group">
                              <label for="exampleFormControlFile1">Upload Additional Document or File</label>
                              <input type="file" class="form-control-file modify" id="exampleFormControlFile1">
                            </div>
                          </fieldset>

                        </form>

                      </form><br>
                      <button class="btn btn-primary" style="float: right" type="subit" id="submit">Publish</button>
                    </fieldset>
                  </div>
                </div>
              </section>
            </div>
          </div>
        </div>
        <div class="card">
          <div class="card-header" id="headingTwo">
            <h5 class="mb-0">
              <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapseTwo"
                aria-expanded="false" aria-controls="collapseTwo">
                Add Event
              </button>
            </h5>
          </div>
          <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
            <div class="card-body">
              <section id="blog_post">
                <br><br>
                <div class="container">
                  <div class="form-group">
                    <fieldset class="the-fieldset">
                      <legend class="the-legend">Update Event</legend>
                      <form method="POST" action="/events" enctype="multipart/form-data">
                        <div class="form-group">
                          <label for="exampleFormControlInput1">Name of the Event</label>
                          <input type="text" class="form-control" id="title" placeholder="Enter your title here.."
                            name="name">
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlInput1">Date(YYYY-MM-DD)</label>
                          <input type="text" class="form-control" id="title"
                            placeholder="Enter your date(YYYY-MM-DD)here.." name="date">
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlInput1">Day</label>
                          <input type="text" class="form-control" id="title" placeholder="Enter the day here.."
                            name="day">
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlInput1">Venue</label>
                          <input type="text" class="form-control" id="title" placeholder="Enter your Venue here.."
                            name="venue">
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlInput1">Hosted By</label>
                          <input type="text" class="form-control" id="title" placeholder="Enter your title here.."
                            name="host">
                        </div>

                        <div class="form-group">
                          <label for="exampleFormControlTextarea1">Enter the description of Event</label>
                          <textarea class="form-control" id="exampleFormControlTextarea1" rows="10"
                            placeholder="Type your blog here" name="description"></textarea>
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlFile1">Hosts's Section</label><br>
                          <label for="exampleFormControlInput1">Name</label>
                          <input type="text" class="form-control" id="title" placeholder="Enter your name here.."
                            name="contact_name">
                          <label for="exampleFormControlInput1">Phone number</label>
                          <input type="text" class="form-control" id="title"
                            placeholder="Enter your Phone number here.." name="contact_phone">
                          <label for="exampleFormControlInput1">Email</label>
                          <input type="text" class="form-control" id="title" placeholder="Enter your Email here.."
                            name="contact_email">
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlFile1">Upload Image</label>
                          <input type="file" class="form-control-file modify" id="exampleFormControlFile1"
                            accept="image/*" name="image">
                        </div>

                        <br><br>
                        <button class="btn btn-primary" style="float: right" type="submit" id="submit">Publish</button>
                      </form>
                    </fieldset>
                  </div>
                </div>
              </section>
            </div>
          </div>
        </div>
